{
	"name": "animendation",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:cloudflare": "vite build --mode cloudflare && npm run clear:sourcemaps",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test": "vitest",
		"client:mal:generate": "openapi --input src/lib/clients/myanimelist/openapi.json --output src/lib/clients/myanimelist/generated",
		"client:jikan:generate": "openapi --input src/lib/clients/jikan/openapi.json --output src/lib/clients/jikan/generated",
		"seed": "vite-node --options.transformMode.ssr=.* src/lib/scripts/seed.ts",
		"migration:generate": "prisma migrate dev",
		"migration:push": "vite-node --options.transformMode.ssr=.* src/lib/scripts/migrate.ts",
		"clear:sourcemaps": "find .svelte-kit/cloudflare -name *.js.map -delete"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20231025.0",
		"@floating-ui/dom": "^1.5.3",
		"@sentry/vite-plugin": "^2.9.0",
		"@sveltejs/adapter-cloudflare": "^2.3.3",
		"@sveltejs/kit": "^1.20.4",
		"@types/jsonwebtoken": "^9.0.3",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"flowbite": "^1.8.1",
		"flowbite-svelte": "^0.44.18",
		"flowbite-svelte-icons": "^0.4.4",
		"openapi-typescript-codegen": "^0.25.0",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prisma": "^5.4.1",
		"prisma-kysely": "^1.7.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vite-node": "^0.34.6",
		"vitest": "^0.34.6"
	},
	"type": "module",
	"dependencies": {
		"@libsql/kysely-libsql": "^0.3.0",
		"@sentry/sveltekit": "^7.77.0",
		"@trpc/client": "^10.38.5",
		"@trpc/server": "^10.38.5",
		"@tsndr/cloudflare-worker-jwt": "^2.2.5",
		"fuse.js": "^6.6.2",
		"idb": "^7.1.1",
		"kysely": "^0.26.3",
		"kysely-neon": "^1.3.0",
		"tailwind-merge": "^1.14.0",
		"toucan-js": "^3.3.1",
		"zod": "^3.22.4"
	},
	"engines": {
		"node": ">=16.0.0"
	}
}
